{
  "testPathDirs": ["src/js/test", "src/js/main"],
  "testRegex" : ".+\\.(?:test|it)\\.es6",
  "moduleFileExtensions" : ["js", "json", "es6"],

  "moduleNameMapper": {
      "^.+\\.(css|less|sass)$": "<rootDir>/src/js/test/testsupport/styleMock.es6",
      "^.+\\.(gif|ttf|eot|svg|png|ico)$": "<rootDir>/src/js/test/testsupport/fileMock.es6"
    },

  "transform": {
    "^.+\\.es6$": "<rootDir>/node_modules/babel-jest"
  }
}
